package arrays;

public class Day4 {
	public static void merge(int arr[], int brr[]) {
		int l1 = arr.length;
		int l2 = brr.length;
		for (int i = 0; i < l1; i++) {
			if (arr[i] < brr[0]) {
				// swap
				int temp = arr[i];
				arr[i] = brr[0];
				brr[0] = arr[i];

				// rearrange the order of brr
				int first = brr[0];
				int k;
				for (k = 1; k < l2 && arr[k] < first; k++) {
					brr[k - 1] = brr[k];
				}
				brr[k - 1] = first;
			}
		}
	}

	public static void main(String[] args) {

	}
}
